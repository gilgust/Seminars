<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <title>Material Design Bootstrap</title>
  <!-- Font Awesome -->
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
  <!-- Bootstrap core CSS -->
  <link asp-href-include="~/thema/css/bootstrap.min.css" rel="stylesheet">
  <!-- Material Design Bootstrap -->
  <link asp-href-include="~/thema/css/mdb.min.css" rel="stylesheet">
  <!-- Your custom styles (optional) -->
  <link asp-href-include="~/thema/css/style.min.css" rel="stylesheet">
  @RenderSection("Head", false)
</head>

<body>

    <!--Main Navigation-->
    <header>

        <!-- Navbar -->
        @Html.Partial("Navbar")
        <!-- Navbar -->

    </header>
    <!--Main Navigation-->
    <!--Main layout-->
    <main class="mt-5 pt-5">
        @RenderBody()
    </main>
    <!--Main layout-->
    <!--Footer-->
    @Html.Partial("Footer")
    <!--/.Footer-->
    <!-- SCRIPTS -->
    <!-- JQuery -->
    <script type="text/javascript" src="~/thema/js/jquery-3.4.1.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="~/thema/js/bootstrap.min.js"></script>
    <!-- Bootstrap tooltips -->
    <script type="text/javascript" src="~/thema/js/popper.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="~/thema/js/mdb.min.js"></script>
    <!-- Initializations -->
    <script type="text/javascript">
        // Animations initialization
        new WOW().init();

    </script>

    @RenderSection("Footer", false)
</body>

</html>
