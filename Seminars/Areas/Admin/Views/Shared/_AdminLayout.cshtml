<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Material Design Bootstrap</title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <!-- Bootstrap core CSS -->
    <link asp-href-include="~/lib/twitter-bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link asp-href-include="~/admin/css/mdb.min.css" rel="stylesheet">
    <!-- Your custom styles (optional) -->
    <link asp-href-include="~/admin/css/style.css" rel="stylesheet">


    @RenderSection("HeadSection", false)
</head>

<body class="grey lighten-3">

    @Html.Partial("Header")

    <!--Main layout-->
    <main class="pt-5 mx-lg-5">

        @RenderBody()

    </main>
    <!--Main layout-->
    <!-- SCRIPTS -->
    <!-- JQuery -->
    <script type="text/javascript" src="~/admin/js/jquery-3.4.1.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="~/admin/js/bootstrap.min.js"></script>
    <!-- Bootstrap tooltips -->
    <script type="text/javascript" src="~/admin/js/popper.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="~/admin/js/mdb.min.js"></script>
    <!-- Initializations -->
    <script type="text/javascript">
        new WOW().init();
    </script>

    @RenderSection("FooterScript", false)

</body>
</html>